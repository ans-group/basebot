{
    "name": "basebot",
    "version": "1.0.0",
    "description": "Virtual Assistant Framework by ANS Group",
    "main": "build/bot.js",
    "license": "MIT",
    "scripts": {
        "start": "node build/bot.js",
        "build": "npm install --only=dev && npx babel ./ --out-dir build --ignore \"node_modules\",\"build\" --copy-files && npm i --prefix build",
        "heroku-prebuild": "npm run build",
        "dev": "nodemon --exec npm run babel-node -- bot.js",
        "babel-node": "babel-node --ignore='node_modules'"
    },
    "dependencies": {
        "@microsoft/microsoft-graph-client": "^1.4.0",
        "azure-storage": "^2.10.2",
        "body-parser": "^1.18.3",
        "botkit": "^0.7.0",
        "botkit-middleware-luis": "^1.0.0",
        "clever-qs": "^7.0.3",
        "cryptr": "^4.0.2",
        "debug": "^3.1.0",
        "dedent": "^0.7.0",
        "firebase-admin": "^6.4.0",
        "html-entities": "^1.2.1",
        "jsonwebtoken": "^8.4.0",
        "lodash": "^4.17.11",
        "moment": "^2.23.0",
        "node-env-file": "^0.1.8",
        "node-schedule": "^1.3.1",
        "querystring": "^0.2.0",
        "request-promise-native": "^1.0.5",
        "simple-oauth2": "^2.2.1",
        "striptags": "^3.1.1",
        "uuid": "^3.3.2",
        "winston": "^2.4.2",
        "winston-papertrail": "^1.0.5"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-import-glob": "^2.0.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^5.10.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-node": "^8.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "localtunnel": "^1.9.1",
        "nodemon": "^1.18.8"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ans-group/basebot.git"
    },
    "bugs": {
        "url": "https://github.com/ans-group/basebot/issues"
