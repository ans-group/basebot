{
  "name": "basebot-core",
  "version": "2.0.2",
  "description": "Virtual Assistant Framework by ANS Group",
  "main": "build/main.js",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "start": "webpack --mode=production && NODE_ENV=production node build/main.js",
    "build": "webpack --mode=production",
    "heroku-prebuild": "npm run build",
    "watch:build": "node-env-run --exec 'webpack --watch'",
    "watch:server": "node-env-run --exec 'nodemon \"./build/main.js\" --watch \"./build\"'",
    "dev": "npx npm-run-all --parallel watch:server watch:build",
    "test": "jest --detectOpenHandles --env=node --silent --forceExit"
  },
  "dependencies": {
    "basebot-auth-microsoft": "^2.0.2",
    "basebot-controller-alexa": "^2.0.2",
    "basebot-logger-debug": "^2.0.2",
    "basebot-logger-papertrail": "^2.0.2",
    "basebot-middleware-registerdevicetoken": "^2.0.2",
    "basebot-storage-azuretables": "^2.0.2",
    "basebot-storage-firestore": "^2.0.2",
    "basebot-util-fcm": "^2.0.2",
    "basebot-util-qnamaker": "^2.0.2",
    "basebot-util-signup": "^2.0.2",
    "botkit": "^0.7.4",
    "botkit-middleware-luis": "^1.0.0",
    "dedent": "^0.7.0",
    "lodash": "^4.17.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ans-group/basebot.git"
  },
  "bugs": {
    "url": "https://github.com/ans-group/basebot/issues"
  },
  "keywords": [
    "bots",
    "chatbots",
