  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-import-glob": "^2.0.0",
    "botkit-mock": "^0.1.11",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "jest": "^24.7.1",
    "localtunnel": "^1.9.1",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.0",
